# openrouter SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: OPENROUTER_
#
# Security environment variables:
OPENROUTER_API_KEY_AUTH=your_apikeyauth_here
OPENROUTER_BEARER_AUTH=your_bearerauth_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
OPENROUTER_DEBUG=false
