{
  "name": "@openrouter/sdk",
  "version": "0.0.1-beta.7",
  "author": "OpenRouter",
  "type": "module",
  "main": "./esm/index.js",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "default": "./esm/index.js"
    },
    "./package.json": "./package.json",
    "./types": {
      "types": "./esm/types/index.d.ts",
      "default": "./esm/types/index.js"
    },
    "./models/errors": {
      "types": "./esm/models/errors/index.d.ts",
      "default": "./esm/models/errors/index.js"
    },
    "./models": {
      "types": "./esm/models/index.d.ts",
      "default": "./esm/models/index.js"
    },
    "./models/operations": {
      "types": "./esm/models/operations/index.d.ts",
      "default": "./esm/models/operations/index.js"
    },
    "./react-query": {
      "types": "./esm/react-query/index.d.ts",
      "default": "./esm/react-query/index.js"
    },
    "./*.js": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenRouterTeam/typescript-sdk.git"
  },
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 src",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tanstack/react-query": "^5.61.4",
    "@types/react": "^18.3.12",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.26.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  }
}